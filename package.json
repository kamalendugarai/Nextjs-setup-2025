{
  "name": "nextshadcn",
  "version": "0.1.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "=20.12.2",
    "npm": "=10.5.0"
  },
  "scripts": {
    "gdown": "pip install gdown",
    "instgdown": "echo '****************************************' && echo 'Current Python Version:\\033[1;33m' $(python --version) && echo '\\033[0;37mRequired Python Version: \\033[1;33mPython' $(cat ./.python-version) && echo '' && echo '\\033[0;37mInstalling \\033[1;33mgdown\\033[0;37m' && echo '\\033[0;37m****************************************' && echo '\\033[0;37mInstalling \\033[1;33mgdown\\033[0;37m' && npm run gdown -s",
    "installgdown": "npm run instgdown -s",
    "copyEnvFiles": "gdown https://drive.google.com/uc\\?id\\=$envfileid -O .env ",
    "currentPythonVersion": "python --version",
    "generateEnvPrompt": "echo '' && echo '\\033[1;33mCopying .env files...\\033[0;37m' && echo '' && npm run copyEnvFiles -s && echo ''",
    "insertfileId": "echo '\\033[1;33mInsert file Id: \\033[0;37m' && read envfileid < /dev/tty && export envfileid && npm run generateEnvPrompt -s",
    "getenv": "npm run insertfileId -s",
    "dev": "next dev",
    "build": "next build --debug",
    "start": "next start",
    "lint": "next lint --fix",
    "knip": "knip",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "posttest": "npm run format",
    "prepare": "husky install",
    "lint:css": "stylelint --fix '**/*.css'",
    "pretest": "npm run lint:css",
    "gitformatstaged": "git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' ."
  },
